<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name="Page Object Model with Page Factories Framework">
    <listeners>
        <listener class-name="org.uncommons.reportng.HTMLReporter"/>
        <listener class-name="org.uncommons.reportng.JUnitXMLReporter"/>
        <listener class-name="page.factories.listeners.CustomListener"/>
    </listeners>

    <test name="Login Test">
        <classes>
            <class name="page.factories.testcases.LoginTest"/>
        </classes>
    </test>
    <test name="Register Test">
        <classes>
            <class name="page.factories.testcases.RegisterAccountTest"/>
        </classes>
    </test>
</suite>